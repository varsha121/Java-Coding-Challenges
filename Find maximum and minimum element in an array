import java.util.*;
public class Main{
  public static void main(String args[]){
    int n[] = {1,3,4,2,7,5,9,8};
    Arrays.sort(n);
    int max=0;
    int min=0;
    System.out.println(Arrays.toString(n));
    for(int i=0; i<n.length-1;i++){
      max = n[0];
      min = n[n.length-1];
    }
    System.out.println(max + " "+ min);
    
    }
}
