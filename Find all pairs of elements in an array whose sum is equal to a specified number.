import java.util.*;
public class Main{
  public static void main(String args[]){
    int n[]={1, 2, 3, 4, 5};
    int el = 4;
    for(int i=0; i<n.length; i++){
      for(int j=0; j<n.length; j++){
        if((n[i]+n[j]) == el ){
          System.out.println(n[i]+" "+n[j]);
        }
      }
    }
  }
}
