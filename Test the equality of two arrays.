public class Main
{
	public static void main(String[] args) {
		int n[] = {1, 2, 3, 4, 5};
		int n2[] = { 1, 2, 7, 4, 5};
		for(int i =0; i<n.length; i++){
		    for(int j=0; j<n2.length; j++){
		        if(n[i] == n2[j]){
		            System.out.println("Two arrays are same");
		        }
		        break;
        }
		}
	}
}
